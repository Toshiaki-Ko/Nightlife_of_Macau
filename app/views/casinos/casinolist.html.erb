<% @casinos = Casino.all %>
<% @casinos.each do |casino| %>
<table class="table-class", border="3">
<tr>

<td class="td-class">
<%= link_to image_tag(casino.image, :size => '290x200'),
 casino_path(:id => casino.id) %>
 </td>

<td>
  名称: <%= link_to casino.casino_name , casino_path(:id => casino.id) %>
</td>

</tr>
</table>
<% end %>